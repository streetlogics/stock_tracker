{
  "name": "back-end",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn run migrate-run-prod && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate-generate": "sh -c 'dotenv -e ../../.env.local -- yarn run typeorm-ts-node-commonjs migration:generate -d src/migrate-data-source.ts src/migrations/$0'",
    "migrate-run": "dotenv -e ../../.env.local -- yarn run typeorm-ts-node-commonjs migration:run -d src/migrate-data-source.ts",
    "migrate-run-prod": "NODE_ENV=production yarn run typeorm migration:run -d dist/migrate-data-source.js"
  },
  "dependencies": {
    "@dataui/crud": "^5.3.1",
    "@dataui/crud-typeorm": "^5.3.1",
    "@nestjs-query/core": "^0.30.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/platform-socket.io": "^10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.4.1",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "moment": "^2.29.4",
    "mysql2": "^3.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^6.0.1",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.5",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.3"
  }
}
